type "ST_MASTER_CONTRACT_REQUESTS_EVENTS" AS TABLE(
    UNIQUE_NAME NVARCHAR(100) NOT NULL,
    EVENT_NO INTEGER,
    EVENT_CODE INTEGER,
    EVENT_TYPE NVARCHAR(20),
    USER_ID NVARCHAR(100),
    USER_NAME NVARCHAR(100),
    REMARK NVARCHAR(100),
    COMMENT NVARCHAR(1000),
    CREATED_ON TIMESTAMP,

    PRIMARY KEY(UNIQUE_NAME,EVENT_NO)
)
